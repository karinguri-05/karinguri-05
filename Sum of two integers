//main class
public class SumOfInteger {
    //main method
    public static void main(String[]args){
        //declaratuon and initialization of variable of integer type
        int X=5;
        int Y=6;
        int sum =0;
        sum =X+Y;
        //output for sum of two integers
        System.out.println("sum of two integers is :" +sum);
    }
    
}
